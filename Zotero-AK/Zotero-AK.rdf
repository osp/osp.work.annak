        <dc:subject>bio:Jeremy Reed</dc:subject>
        <dc:subject>publisher:Peter Owen</dc:subject>
        <dc:title>A stranger on Earth: the life and work of Anna Kavan</dc:title>
        <dc:date>2006</dc:date>
        <z:numPages>207</z:numPages>
        <dc:identifier>ISBN 072061273X</dc:identifier>
        <z:shortTitle>A stranger on Earth</z:shortTitle>
        <z:libraryCatalog>Library of Congress ISBN</z:libraryCatalog>
